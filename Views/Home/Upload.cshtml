@{
    ViewData["Title"] = "Upload";
}
<h1>File Upload</h1>


<form method="post" enctype="multipart/form-data">
    <input type="file" name="file"/>
    <button type="submit">Upload</button>
</form>

@if(ViewBag.Message != null){
    <div class="alert alert-success" style="margin-top:20px">
        @ViewBag.Message
    </div>
}